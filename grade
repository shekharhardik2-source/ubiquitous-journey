#include <stdio.h>

int main() {
    float marks[5];
    float total = 0;
    for(int i = 0; i < 5; i++) {
        printf("Enter marks for subject %d: ", i+1);
        scanf("%f", &marks[i]);
        total += marks[i];
    }
     float percentage = total / 5.0;

    // Determine grade
    char grade;
    if(percentage >= 90)
        grade = 'A';
    else if(percentage >= 80)
        grade = 'B';
    else if(percentage >= 70)
        grade = 'C';
    else if(percentage >= 60)
        grade = 'D';
    else
        grade = 'F';
    printf("Percentage: %f\n", percentage);
    printf("Grade: %c\n", grade);

    return 0;
}